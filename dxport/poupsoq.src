				!Let OL7[3]=0
				if ol7[4]<>0 ! has a ship qty
					let ol7[3]=ol7[2]-ol7[0]-qtyrecv-prevqty ! backorder is qty order- qty prev ship - qtyrecv
				else
					LET OL7[3]=OL7[3]-QTYRECV
				endif
				if ol7[3]<0 let ol7[3]=0
			end if
  		Else ! "did not recv all
				if ol7[4]<>0
					let ol7[3]=ol7[2]-ol7[0]-qtyrecv-prevqty ! backorder is qty order- qty prev ship - qtyrecv
				else
					Let OL7[3]=OL7[3]-QtyRecv
